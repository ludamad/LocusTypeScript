    "files": [
        "./error_tests.ts",
        "./multifile/first.ts",
        "./multifile/second.ts",
        "./nullable.ts",
        "./runtime_tests.ts",
        "./sep.ts",
        "./temp.ts",
        "./test.ts",
        "./this_test.ts"
    ]
}
